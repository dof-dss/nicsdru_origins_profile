{
  "name": "dof-dss/nicsdru_origins_profile",
  "description": "Drupal install profile to provide common functionality for NICS websites",
  "type": "project",
  "authors": [
    {
      "name": "Martin Dutton"
    }
  ],
  "repositories": [
    {
      "type": "github",
      "url": "git@github.com:dof-dss/nicsdru_origins_modules.git",
      "no-api": true
    },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "dof-dss/nicsdru_origins_modules": "dev-development",
    "drupal/admin_toolbar": "^1.2",
    "drupal/address": "^1.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "modules/origins": [
        "dof-dss/nicsdru_origins_modules"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ]
    }
  }
}
